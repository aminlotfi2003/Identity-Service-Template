{
    "ConnectionStrings": {
        "Default": ""
    },
    "Seed": {
        "Enable": true,
        "AdminEmail": "admin@local",
        "AdminUserName": "admin",
        "AdminPassword": "",
        "AdminRoleName": "Admin",
        "DefaultRoles": [ "Reader", "Contributor", "Approver", "Admin" ],
        "DefaultPermissions": [ "Users.Read", "Users.Write", "Roles.Read", "Roles.Write", "Permissions.Read", "Permissions.Write" ],
        "DefaultDataLabels": [ "INTERNAL", "CONFIDENTIAL", "RESTRICTED" ]
    },
    "Identity": {
        "Cookie": {
            "Name": ".ids.auth",
            "LoginPath": "/account/login",
            "LogoutPath": "/account/logout",
            "AccessDeniedPath": "/errors/403",
            "ExpireMinutes": 20
        },
        "Tokens": {
            "DefaultLifespanMinutes": 120
        }
    }
}
